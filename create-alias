#---------#
# Aliases #
#---------#

### Vim
alias lv='vim -u ~/.setup/lauren-vimrc'

### Git
alias gst='git status'
alias gd='git diff'
alias gs='git show'
alias gco='git checkout'
alias ga='git add'
alias gaa='git add -A'
alias gcm='git commit -m'
alias gca='git commit --amend'
alias gp='git push origin'
alias gl='git log'
alias gri='git rebase -i'
